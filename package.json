{
  "name": "tuesplace",
  "version": "0.5.5",
  "description": "Server for tuesplace",
  "main": "./src/index.ts",
  "engines": {
    "node": "16.17.0",
    "npm": "8.19.2"
  },
  "engineStrict": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .ts",
    "format": "eslint src --ext .ts --fix",
    "start:dev": "nodemon ./src/index.ts",
    "prebuild": "npm run lint",
    "build": "tsc",
    "start": "node ./dist/index.js",
    "postbuild": "copyfiles -u 1 src/**/*.yaml dist/",
    "clean": "npx rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuesplace/tuesplace.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tuesplace/tuesplace/issues"
  },
  "homepage": "https://github.com/tuesplace/tuesplace#readme",
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.223.0",
    "@aws-sdk/client-s3": "^3.223.0",
    "@aws-sdk/cloudfront-signer": "^3.222.0",
    "async-middleware": "^1.2.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.7.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mustache": "^4.2.0",
    "nodemailer": "^6.8.0",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.6.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.189",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/mustache": "^4.2.1",
    "@types/node": "^18.11.9",
    "@types/nodemailer": "^6.4.6",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
